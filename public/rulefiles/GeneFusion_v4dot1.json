{
    "VariantFilter": [
        {
            "SVTYPE": {
                "Operator1": "==",
                "Value1": "Fusion"
            }
        }
    ],
    "PassFilter": [
        {
            "FILTER": {
                "Operator1": "==",
                "Value1": "PASS"
            }
        }
    ],
    "GeneFusionReadDepthFilter": [
        {
            "DP": {
                "Operator1": ">=",
                "Value1": "25"
            }
        }
    ],
    "GeneFusionEGFRReadDepthFilter": [
        {
            "and": [
                {
                    "ID": {
                        "Operator1": "startsWith",
                        "Value1": "EGFR-EGFR.E1E8.DelPositive"
                    }
                },
                {
                    "DP": {
                        "Operator1": ">=",
                        "Value1": "1000"
                    }
                }
            ]
        }
    ],
    "IdentifierFilter": [
        {
            "and": [
                {
                    "ID": {
                        "Operator1": "!=",
                        "Value1": ""
                    }
                },
                {
                    "ID": {
                        "Operator1": "!=",
                        "Value1": "."
                    }
                }
            ]
        }
    ]
}